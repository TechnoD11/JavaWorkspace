import java.util.Scanner;

/**
 * 
 * @author Derek Wider
 * Programming Assignment 4
 *
 */
public class primeCalculator {
	public static void main(String[] args){
		Scanner in = new Scanner(System.in);
		System.out.println("Welcome to the prime calculator! Please input to what number you wish to calculate prime numbers to, ex. to 100,000");
		int primeLimit = in.nextInt();
		int ROW_LIMIT = 6;
		int numberOfPrimes = 0;
		int rowCounter = 0;
		boolean isPrime = true;
		for(int i = 1; i <= primeLimit; i++){
			isPrime = true;
			for(int j = 2; (j <= (i/2)) && isPrime; j++){
				if(i % j == 0){
					isPrime = false;
				}
			}
			if(isPrime){
				numberOfPrimes++;
				if(rowCounter != ROW_LIMIT){
					System.out.print(i + " ");
					rowCounter++;
				}
				else{
					System.out.println(i);
					rowCounter = 0;
				}
			}
		}
		/*	
		if(isPrime){
			System.out.println("The number");
		}
		System.out.println("The number is ");
	*/}
}
