public class FlipDemo {

	public static void main(String[] args) {
		TicTacToe game = new TicTacToe();
		/*game.set(0, 0, "o");
		game.set(0, 1, "o");*/
		game.set(0, 2, "x");
		/*game.set(1, 0, "o");
		game.set(2, 1, "x");*/
		System.out.println(game.toString());
		System.out.println(game.flipVertical());
	}

}
