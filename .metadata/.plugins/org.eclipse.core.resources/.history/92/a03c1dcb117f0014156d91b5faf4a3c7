/**
 * 
 * @author Derek Wider and Nick Napior
 *
 */
public class MagicSquareRunner {

	public static void main(String[] args) {
		int[][] myints = new int[3][4];
		for(int i = 0; i < 3; i++){
			for(int j = 0; j < 4; j++){
				myints[i][j] = j * i;
			}
		}
		System.out.println(myints[3][2]);
	}

}
